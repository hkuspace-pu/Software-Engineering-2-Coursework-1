<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="409dp"
        android:layout_height="match_parent"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:orientation="horizontal">

            <FrameLayout
                android:layout_width="313dp"
                android:layout_height="match_parent">

            </FrameLayout>

            <FrameLayout
                android:layout_width="48dp"
                android:layout_height="match_parent">

                <ImageButton
                    android:id="@+id/alarmBtn"
                    android:layout_width="48dp"
                    android:layout_height="48dp"
                    android:backgroundTint="@color/white"
                    android:scaleType="centerCrop"
                    app:srcCompat="@drawable/alarm"
                    tools:layout_editor_absoluteX="257dp"
                    tools:layout_editor_absoluteY="236dp" />
            </FrameLayout>

            <FrameLayout
                android:layout_width="48dp"
                android:layout_height="match_parent">

                <ImageButton
                    android:id="@+id/infoBtn"
                    android:layout_width="48dp"
                    android:layout_height="48dp"
                    android:backgroundTint="@color/white"
                    android:scaleType="centerCrop"
                    app:srcCompat="@drawable/anna_icon"
                    tools:layout_editor_absoluteX="305dp"
                    tools:layout_editor_absoluteY="236dp" />
            </FrameLayout>
        </LinearLayout>

        <TableLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:id="@+id/textView11"
                    android:layout_width="409dp"
                    android:layout_height="60dp"
                    android:text="My Dashboard"
                    android:textAlignment="center"
                    android:textSize="34sp"
                    android:textStyle="bold"
                    tools:layout_editor_absoluteX="46dp"
                    tools:layout_editor_absoluteY="284dp" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <ImageView
                    android:id="@+id/imageView4"
                    android:layout_width="319dp"
                    android:layout_height="175dp"
                    app:srcCompat="@drawable/admin_user"
                    tools:layout_editor_absoluteX="46dp"
                    tools:layout_editor_absoluteY="369dp" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:id="@+id/textView12"
                    android:layout_width="170dp"
                    android:layout_height="49dp"
                    android:text="Administrator"
                    android:textAlignment="center"
                    android:textSize="20sp"
                    tools:layout_editor_absoluteX="120dp"
                    tools:layout_editor_absoluteY="544dp" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:id="@+id/textView13"
                    android:layout_width="170dp"
                    android:layout_height="49dp"
                    android:text="Anna Tsui"
                    android:textAlignment="center"
                    android:textSize="24sp"
                    android:textStyle="bold"
                    tools:layout_editor_absoluteX="120dp"
                    tools:layout_editor_absoluteY="596dp" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="72dp">

                </FrameLayout>
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <LinearLayout
                    android:layout_width="406dp"
                    android:layout_height="165dp"
                    android:orientation="horizontal"
                    tools:layout_editor_absoluteX="1dp"
                    tools:layout_editor_absoluteY="440dp">

                    <FrameLayout
                        android:layout_width="135dp"
                        android:layout_height="match_parent">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical">

                            <ImageButton
                                android:id="@+id/recBtn"
                                android:layout_width="match_parent"
                                android:layout_height="89dp"
                                android:backgroundTint="@color/white"
                                android:scaleType="centerInside"
                                app:srcCompat="@drawable/employee_record"
                                tools:layout_editor_absoluteX="36dp"
                                tools:layout_editor_absoluteY="545dp" />

                            <TextView
                                android:id="@+id/textView6"
                                android:layout_width="match_parent"
                                android:layout_height="52dp"
                                android:text="Employee's Record"
                                android:textAlignment="center"
                                android:textSize="20sp"
                                tools:layout_editor_absoluteX="16dp"
                                tools:layout_editor_absoluteY="634dp" />
                        </LinearLayout>
                    </FrameLayout>

                    <FrameLayout
                        android:layout_width="135dp"
                        android:layout_height="match_parent">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical">

                            <ImageButton
                                android:id="@+id/leaveApprovalBtn"
                                android:layout_width="match_parent"
                                android:layout_height="89dp"
                                android:backgroundTint="@color/white"
                                android:scaleType="centerInside"
                                app:srcCompat="@drawable/calender"
                                tools:layout_editor_absoluteX="159dp"
                                tools:layout_editor_absoluteY="545dp" />

                            <TextView
                                android:id="@+id/textView3"
                                android:layout_width="match_parent"
                                android:layout_height="52dp"
                                android:layout_marginBottom="45dp"
                                android:text="Leave Approval"
                                android:textAlignment="center"
                                android:textSize="20sp"
                                app:layout_constraintBottom_toBottomOf="parent"
                                app:layout_constraintTop_toBottomOf="@+id/leaveApprovalBtn"
                                tools:layout_editor_absoluteX="142dp" />
                        </LinearLayout>
                    </FrameLayout>

                    <FrameLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical">

                            <ImageButton
                                android:id="@+id/newEmployeeBtn"
                                android:layout_width="match_parent"
                                android:layout_height="89dp"
                                android:backgroundTint="@color/white"
                                android:scaleType="centerInside"
                                app:srcCompat="@drawable/new_employee"
                                tools:layout_editor_absoluteX="285dp"
                                tools:layout_editor_absoluteY="545dp" />

                            <TextView
                                android:id="@+id/textView5"
                                android:layout_width="match_parent"
                                android:layout_height="52dp"
                                android:text="New Employee"
                                android:textAlignment="center"
                                android:textSize="20sp"
                                tools:layout_editor_absoluteX="278dp"
                                tools:layout_editor_absoluteY="634dp" />
                        </LinearLayout>
                    </FrameLayout>
                </LinearLayout>
            </TableRow>

        </TableLayout>
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>